{
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk (lualatex + biber)",
      "tools": [
        "latexmk-lualatex"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk-lualatex",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-interaction=nonstopmode",
        "-synctex=1",
        "-lualatex",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.latex.recipe.default": "latexmk (lualatex + biber)",
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bcf",
    "*.ilg",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.nlo",
    "*.nls*",
    "*.run.xml",
    "*.fdb_latexmk"
  ],
  "ltex.enabled": true,
  "ltex.language": "de-DE",
  "ltex.hiddenFalsePositivesFile": ".vscode/ltex.hiddenFalsePositives.de-DE.txt",
  "ltex.dictionaryFolder": ".vscode"
}
